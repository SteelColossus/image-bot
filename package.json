{
  "name": "image-bot",
  "version": "1.0.0",
  "description": "A Discord bot which posts images from user searches.",
  "main": "image-bot.ts",
  "author": "steelcolossus",
  "repository": {
    "type": "git",
    "url": "https://github.com/SteelColossus/image-bot.git"
  },
  "private": true,
  "scripts": {
